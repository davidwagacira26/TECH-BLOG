<%- include('layout', { content: `
    <h1>Welcome to Tech Blog</h1>
    <% posts.forEach(post => { %>
        <article>
            <h2><%= post.title %></h2>
            <p><%= post.content %></p>
            <small>Posted on: <%= new Date(post.created_at).toLocaleDateString() %></small>
        </article>
    <% }) %>
` }) %>
